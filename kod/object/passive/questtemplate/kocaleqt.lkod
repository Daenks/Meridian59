% Quest description.
kocaleqt_name_rsc = de "Ko'catan ale trade quest"
kocaleqt_desc_rsc = de \
   "Make an alcohol delivery to the Island."

kocaleqt_nodetwo_rsc = de \
   "%s%s needs a guaranteed delivery made to the island.  Failure to "
   "make the delivery would be punished. Will you accept these terms?"

kocaleqt_nodethree_rsc = de \
   "Deliver %i %s to %s%s.  Do not be tempted to keep the supplies "
   "for yourself; thievery will be punished."

kocaleqt_completed_rsc = de \
   "You have completed the Ko'catan weapon trade quest."

% Quest resources.
kocale_assign_2 = de \
   "Ich brauche eine garantierte Zustellung zum Festland.  Ein Versagen führt "
